{
	"extends": "astro/tsconfigs/strict",
	"compilerOptions": {
		"jsx": "react-jsx",
		"jsxImportSource": "react",
		"allowJs": true,

		"baseUrl": ".",
		"paths": {
			"@components/*": ["src/components/*"],
			"@content/*": ["src/content/*"],
			"@images/*": ["src/content/images/*"],
			"@layouts/*": ["src/layouts/*"]
		}
	}
}
